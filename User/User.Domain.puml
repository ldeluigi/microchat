@startuml User Domain
!include meta/Domain.Analysis.metamodel.iuml

$aggregate(User) {
    $aggregate_root(User) {
        + id: Guid
        + name: String
        + surname: String
        + username: Username
        + email: String
        + creationTime: DateTime
        + lastLoginTime: Option[DateTime]
        + updateEmail(String)
        + updateUsername(Username)
    }

    $value(Username) {
        + name: String
    }
    
    Username -o User
}
@enduml