@startuml User Domain
!include meta/Domain.Analysis.metamodel.iuml

$aggregate(User) {
    $aggregate_root(User) {
        + id: Guid
        + name: Name
        + surname: Name
        + email: Email
        + updateName(Name)
        + updateSurname(Name)
    }

    $value(Name) {
        + value: String
    }

    $value(Email) {
        + value: String
    }
    
    Name -o User
    Email -o User
}
@enduml