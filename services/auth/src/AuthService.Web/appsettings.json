{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AzureServiceBusSettings": {
    "TopicName": "global"
  },
  "JwtSettings": {
    "Global": {
      "Lifetime": "00:05:00"
    }
  },
  "TokensSettings": {
    "RefreshTokenLifetime": "90.00:00:00",
    "PasswordTokenLifetime": "0.02:00:00",
    "AccountVerificationTokenLifetime": "1.00:00:00"
  },
  "Migrations": {
    "RunOnStartup": false
  }
}
